// need to look at this later

 public fun combine_nfts(
        nft1: NonFungibleToken,
        nft2: NonFungibleToken,
        new_image_url: vector<u8>,
        ctx: &mut TxContext,
    ): NonFungibleToken {

        let name = string::utf8(b"");
        let description = string::utf8(b"Combined NFT of ");
       
        string::append(&mut name, nft1.name);
        string::append(&mut name, string::utf8(b" + "));
        string::append(&mut name, nft2.name);    
        string::append(&mut description, nft1.name);
        string::append(&mut description, string::utf8(b" and "));
        string::append(&mut description, nft2.name);


       // let length = vector::length(&name);
        //let result = string::empty();
        
       
        let new_nft = NonFungibleToken{
            id: object::new(ctx), 
            name,
            description,
            image: url::new_unsafe_from_bytes(new_image_url),
        };
       let nonFunginbleCombined = NonFungibleTokenCombined{
            nft1_id : *object::uid_as_inner(&nft1.id),
            nft2_id : *object::uid_as_inner(&nft2.id),
            new_nft_id:  *object::uid_as_inner(&new_nft.id),
        };
        
        event::emit(nonFunginbleCombined);

        let NonFungibleToken {
            id, 
            name:_, 
            description:_, 
            image:_} = nft2; 
        object::delete(id);
        let NonFungibleToken {
            id, 
            name:_, 
            description:_, 
            image:_} = nft1;
        object::delete(id);
        
        new_nft
        
    }
